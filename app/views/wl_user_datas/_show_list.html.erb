		<div class="autoscroll">
		<table class="list">
			<thead><tr>
				<th><%= l(:workload_show_filter_user) %></th>
				<th><%= l(:workload_settings_hours_low_min) %></th>
				<th><%= l(:workload_settings_hours_normal_min) %></th>
				<th><%= l(:workload_settings_hours_high_min) %></th>
				<% if is_allowed %>			
					<th colspan="2"></th>
				<% end %>
			</tr></thead>
			<tbody>
		<% for data in wl_users_data -%>
			<tr class="<%= cycle("odd", "even") %>">
				<td><%= data.user.name.to_s %></td>
				<td><%= data.threshold_lowload_min.to_s %></td>
				<td><%= data.threshold_normalload_min.to_s %></td>
				<td><%= data.threshold_highload_min.to_s %></td>			
				<% if is_allowed %>
	      			<td><%= link_to l(:button_edit), edit_wl_user_data_path(data) %></td>
	      			<td><%= link_to l(:button_delete), wl_user_data_path(data), method: :delete, data: { confirm: l(:text_are_you_sure) } %></td>
	            <% end %>
			</tr>
		<% end -%>	
			</tbody>
		</table>